{
    "manifest_version": 3,
    "name": "Video Manager",
    "version": "1.0",
    "description": "Gérer les vidéos sur différents sites.",
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }    
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  